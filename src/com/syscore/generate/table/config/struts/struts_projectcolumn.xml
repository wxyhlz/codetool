<?xml version="1.0" encoding="UTF-8" ?>
	<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
	<struts>
	<package name="projectColumnAction" extends="centuryStruts" namespace="/generateproject">

		<!-- 数据列窗体 -->
		<action name="projectColumnForm">
			<result name="success">/WEB-INF/generate/projectColumn.jsp</result>
		</action>
		
		
		<!-- 数据查询 -->
		<action name="findProjectColumnByMap" class="projectColumnAction" method="findProjectColumnByMap">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 刷新 -->
		<action name="updateRefresh" class="projectColumnAction" method="updateRefresh">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 删除数据 -->
		<action name="deleteProjectColumn" class="projectColumnAction" method="deleteProjectColumn">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>
		
		<!-- 自动生成功能模块文件 -->
		<action name="updateGenerateFile" class="projectColumnAction" method="updateGenerateFile">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>
 
	</package>
</struts>
