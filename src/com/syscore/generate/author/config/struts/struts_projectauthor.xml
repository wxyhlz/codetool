<?xml version="1.0" encoding="UTF-8" ?>
	<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
	<struts>
	<package name="projectauthorAction" extends="centuryStruts" namespace="/projectAuthor">

		<!-- 研发人员窗体 -->
		<action name="projectauthorForm">
			<result name="success">/WEB-INF/generate/projectAuthor.jsp</result>
		</action>

		<!-- 添加数据 -->
		<action name="insertProjectauthor" class="projectauthorAction" method="insertProjectauthor">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 删除数据 -->
		<action name="deleteProjectauthor" class="projectauthorAction" method="deleteProjectauthor">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 修改数据 -->
		<action name="updateProjectauthor" class="projectauthorAction" method="updateProjectauthor">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 分页查询 -->
		<action name="findProjectauthorByPage" class="projectauthorAction" method="findProjectauthorByPage">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action> 

	</package>
</struts>
