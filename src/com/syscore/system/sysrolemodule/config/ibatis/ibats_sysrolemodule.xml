<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="SysrolemoduleInfo">
	<typeAlias alias="sysrolemoduleInfo" type="com.syscore.system.sysrolemodule.model.SysrolemoduleInfo" />
	<resultMap id="sysrolemoduleResult" class="com.syscore.system.sysrolemodule.model.SysrolemoduleInfo">
		<!-- ID -->
		<result property="id" column="id" />
		<!-- 角色ID -->
		<result property="rid" column="rid" />
		<!-- 模块ID -->
		<result property="mid" column="mid" />
	</resultMap>

	<!-- 数据添加 -->
	<insert id="insert" parameterClass="sysrolemoduleInfo">
	    <![CDATA[ 
	    	insert into s_sysrolemodule(
				id
				,rid
				,mid
			)
			values(
				#id#
				,#rid#
				,#mid#
			)
		]]>
	</insert>

	<!-- 数据删除 -->
	<delete id="delete" parameterClass="sysrolemoduleInfo">
		<![CDATA[
			delete from s_sysrolemodule where id = #id#
		]]>
    </delete>
    
    
	<!-- 数据查询 -->
	<select id="selectEntitys" parameterClass="sysrolemoduleInfo" resultMap="sysrolemoduleResult" >
		select * from s_sysrolemodule 
		<dynamic prepend="where">
			<isNotEmpty  property="id" prepend="id">
	              id = #id#
	        </isNotEmpty >
	        <isNotEmpty  property="rid" prepend="and">
	              rid = #rid#
	        </isNotEmpty >
	        <isNotEmpty  property="mid" prepend="and">
	              mid = #mid#
	        </isNotEmpty >
       	</dynamic>
		
	</select>
 

</sqlMap>
