<?xml version="1.0" encoding="UTF-8" ?>
	<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
	<struts>
	<package name="operatorAction" extends="centuryStruts" namespace="/operator">

		<!-- 用户登陆 -->
		<action name="findLogin" class="operatorAction" method="findLogin">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 用户所能操作的模块 -->
		<action name="findModules" class="operatorAction" method="findModules">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 账号退出 -->
		<action name="findOut" class="operatorAction" method="findOut">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action> 

	</package>
</struts>
