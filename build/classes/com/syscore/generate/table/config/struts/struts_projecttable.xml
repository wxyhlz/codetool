<?xml version="1.0" encoding="UTF-8" ?>
	<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
	<struts>
	<package name="projectTableAction" extends="centuryStruts" namespace="/generateproject">
		
		<!-- 数据表窗体 -->
		<action name="projectTableForm">
			<result name="success">/WEB-INF/generate/projectTable.jsp</result>
		</action>
		 

		<!-- 添加数据 -->
		<action name="insertProjectTable" class="projectTableAction" method="insertProjectTable">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 删除数据 -->
		<action name="deleteProjectTable" class="projectTableAction" method="deleteProjectTable">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 修改数据 -->
		<action name="updateProjectTable" class="projectTableAction" method="updateProjectTable">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action>

		<!-- 分页查询 -->
		<action name="findProjectTableByPage" class="projectTableAction" method="findProjectTableByPage">
			<result type="json">
				<param name="contentType">text/html</param>
				<param name="root">badusResponse</param>
			</result>
		</action> 

	</package>
</struts>
